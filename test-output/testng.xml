  </test>
   <test name="Test-Resources-Delete">
    <classes>
      <class name="roomManager.test.admin.CreateResource" />
    </classes>
  </test>    
   <test name="Test-Resources-Edit">
    <classes>
      <class name="roomManager.test.admin.EditResource" />
    </classes>
  </test>
   <test name="Test-Location-Create">
    <classes>
      <class name="roomManager.test.admin.CreateLocation" />
    </classes>
  </test>
</suite>